<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="questionnaire.database.QChooseResult" table="q_choose_result">
        <id name="chooseResultId" type="java.lang.Integer" column="choose_result_id">
<!--            what is the label mean?-->
            <generator class="identity" />
        </id>
<!--        <many-to-one name="parentQ" class="questionnaire.database."-->
        <set name="chosenOrders" table="chosen_result">
            <key column="belongs" />
            <one-to-many class="questionnaire.database.ChosenResult" />
        </set>
    </class>
</hibernate-mapping>