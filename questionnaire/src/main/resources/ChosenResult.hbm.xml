<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="questionnaire.database.ChosenResult" table="chosen_result">
        <id name="orders" type="java.lang.Integer">
            <column name="orders" />
            <generator class="identity" />
        </id>

        <many-to-one name="parentChosenResult" class="questionnaire.database.QChooseResult" column="belongs"/>
    </class>
</hibernate-mapping>